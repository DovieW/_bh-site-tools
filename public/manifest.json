{
  "manifest_version": 3,
  "key" :"",
  "version": "0.1.12",
  "name": "B&H Site Tools",
  "description": "Various tools for the the B&H website",
  "action": {
    "default_popup": "index.html",
    "default_title": "B&H Site Tools"
  },
  "permissions": [
    "cookies",
    "tabs",
    "storage",
    "downloads",
    "contextMenus",
    "browsingData",
    "scripting"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "icons": {
    "128": "logo128.png"
  },
  "commands": {
    "_execute_action": {
      "suggested_key": {
        "default": "Alt+Z"
      }
    },
    "duplicate-tab": {
      "suggested_key": "Alt+Shift+D",
      "description": "Duplicate tab"
    },
    "pin-tab": {
      "suggested_key": "Alt+Shift+Z",
      "description": "Pin tab"
    },
    "go-incognito": {
      "suggested_key": "Alt+Shift+N",
      "description": "Open tab in incognito"
    },
    "expand-collapse-tools": {
      "description": "Expand/collapse tools"
    }
  },
  "background": {
    "service_worker": "background.js"
  },
  "incognito": "split"
}
